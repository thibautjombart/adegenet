@ARTICLE{tj828,
  author = {O. G. Pybus and E. C. Holmes and P. H. Harvey},
  title = {The mid-depth method and HIV-1: a practical approach for testing
	hypotheses of viral epidemic history.},
  journal = {Mol Biol Evol},
  year = {1999},
  volume = {16},
  pages = {953--959},
  number = {7},
  month = {Jul},
  abstract = {We introduce the mid-depth method, a practical approach for testing
	hypotheses of demographic history using genealogies reconstructed
	from sequence data. The relative positions of internal nodes within
	a genealogy contain information about past population dynamics. We
	explain how this information can be used to (1) test the null hypothesis
	of constant population size and (2) estimate the growth rate and

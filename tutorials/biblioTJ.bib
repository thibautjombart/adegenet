@ARTICLE{tj613,
  author = {McKechnie, Stephen W. and Ehrlich, Paul R. and White, Raymond R.},
  title = {POPULATION GENETICS OF EUPHYDRYAS BUTTERFLIES. {I}. GENETIC VARIATION

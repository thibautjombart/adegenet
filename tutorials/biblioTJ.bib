@ARTICLE{tj774,
  author = {N. Galtier and B. Nabholz and S. Glémin and G. D D Hurst},
  title = {Mitochondrial DNA as a marker of molecular diversity: a reappraisal.},
  journal = {Mol Ecol},
  year = {2009},
  month = {Oct},
  abstract = {Abstract Over the last three decades, mitochondrial DNA has been the
	most popular marker of molecular diversity, for a combination of
	technical ease-of-use considerations, and supposed biological and
	evolutionary properties of clonality, near-neutrality and clock-like
	nature of its substitution rate. Reviewing recent literature on the
	subject, we argue that mitochondrial DNA is not always clonal, far
	from neutrally evolving and certainly not clock-like, questioning
	its relevance as a witness of recent species and population history.
	We critically evaluate the usage of mitochondrial DNA for species
	delineation and identification. Finally, we note the great potential
	of accumulating mtDNA data for evolutionary and functional analysis
	of the mitochondrial genome.},
